<h1 align="center"> Component Inventory Management </h1>

<div>

<table class="table table-bordered">
  <thead class="thead-dark  ">
    <tr>
      <th scope="col">#</th>
      <th scope="col">FirstName</th>
      <th scope="col">LastName</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Status</th>
     
      <th scope="col" class="text-center">Action</th>
    </tr>
  </thead>
  <tbody>

    {{#each rows}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.first_name}}</td>
      <td>{{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td>{{this.status}}</td>
     
      <td class="text-center">

        <a href='#' type="button" class="btn btn-success btn-small"><i  class="bi bi-eye"></i>View </a>
        <a href='#' type="button" class="btn btn-primary btn-small"><i  class="bi bi-pencil"></i>Edit </a>
        <a href='#' type="button" class="btn btn-danger btn-small"><i  class="bi bi-trash-fill"></i>Delete </a>
        
      </td>
    </tr>
   {{/each}}
   
  </tbody>
</table>
</div>